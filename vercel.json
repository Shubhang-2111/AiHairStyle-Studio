{
    "builds": [
      {
        "src": "api/main.py",
        "use": "@vercel/python"
      },
      {
        "src": "frontend.py",
        "use": "@vercel/python"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "/api/main.py"
      },
      {
        "src": "/(.*)",
        "dest": "/frontend.py"
      }
    ]
  }
  